<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<!-- The Xsp Library -->
	<extension point="com.ibm.commons.Extension">
		<service type="com.ibm.xsp.Library" class="org.openntf.xsp.groovyshell.GroovyShellLibrary" />
	</extension>
	
	<!-- The ApplicationListener to init the server -->
	<extension point="com.ibm.commons.Extension">
		<service type="com.ibm.xsp.core.events.ApplicationListener" class="org.openntf.xsp.groovyshell.ApplicationListener" />
	</extension>
</plugin>
